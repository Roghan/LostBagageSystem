<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.MenuButton?>
<?import javafx.scene.control.RadioButton?>
<?import javafx.scene.control.RadioMenuItem?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.ToggleGroup?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>

<AnchorPane id="AnchorPane" styleClass="mainFxmlClass" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.mycompany.lostbagagesystem.Controllers.ExcelImportExportController">
    <stylesheets>
        <URL value="@/styles/Styles.css" />
    </stylesheets>
   <children>
      <BorderPane AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <top>
            <HBox BorderPane.alignment="CENTER">
               <BorderPane.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </BorderPane.margin>
               <children>
                  <TextField fx:id="txtFilter" alignment="TOP_LEFT" onAction="#dbTableFill" prefHeight="25.0" prefWidth="711.0" />
                  <MenuButton fx:id="filterBox" mnemonicParsing="false" text="MenuButton">
                    <items>
                        <RadioMenuItem mnemonicParsing="false" onAction="#setTxtFilter" text="%TLK36">
                           <toggleGroup>
                              <ToggleGroup fx:id="txtBoxFilter" />
                           </toggleGroup>
                        </RadioMenuItem>
                        <RadioMenuItem mnemonicParsing="false" onAction="#setTxtFilter" text="%TLK130" toggleGroup="$txtBoxFilter" />
                        <RadioMenuItem mnemonicParsing="false" onAction="#setTxtFilter" text="%TLK131" toggleGroup="$txtBoxFilter" />
                        <RadioMenuItem mnemonicParsing="false" onAction="#setTxtFilter" text="%TLK10" toggleGroup="$txtBoxFilter" />
                    </items>
                     <HBox.margin>
                        <Insets left="10.0" />
                     </HBox.margin>
                  </MenuButton>
                  <RadioButton fx:id="filterLost" mnemonicParsing="false" onAction="#dbTableFill" text="%TLK136" textAlignment="CENTER">
                     <toggleGroup>
                        <ToggleGroup fx:id="Filter" />
                     </toggleGroup>
                     <HBox.margin>
                        <Insets left="10.0" right="10.0" top="4.0" />
                     </HBox.margin>
                  </RadioButton>
                  <RadioButton fx:id="filterFound" mnemonicParsing="false" onAction="#dbTableFill" text="%TLK137" textAlignment="CENTER" toggleGroup="$Filter">
                     <HBox.margin>
                        <Insets left="10.0" right="10.0" top="4.0" />
                     </HBox.margin>
                  </RadioButton>
                  <RadioButton fx:id="filterAll" mnemonicParsing="false" onAction="#dbTableFill" selected="true" text="%TLK138" textAlignment="CENTER" toggleGroup="$Filter">
                     <HBox.margin>
                        <Insets left="10.0" right="10.0" top="4.0" />
                     </HBox.margin>
                  </RadioButton>
                  <Button mnemonicParsing="false" onAction="#importExcelFile" text="%TLK153" />
                  <Button mnemonicParsing="false" onAction="#importTable" text="%TLK154">
                     <HBox.margin>
                        <Insets left="10.0" right="10.0" />
                     </HBox.margin>
                  </Button>
               </children>
            </HBox>
         </top>
         <center>
            <TableView fx:id="bagage">
              <columns>
                <TableColumn fx:id="registrationNr" maxWidth="1.7976931348623157E308" minWidth="-1.0" prefWidth="75.0" text="%TLK139" />
                <TableColumn fx:id="dateFound" maxWidth="1.7976931348623157E308" minWidth="-1.0" prefWidth="75.0" text="%TLK140" />
                  <TableColumn fx:id="timeFound" maxWidth="1.7976931348623157E308" minWidth="-1.0" prefWidth="95.0" text="%TLK141" />
                  <TableColumn fx:id="luggageType" maxWidth="1.7976931348623157E308" minWidth="-1.0" prefWidth="72.0" text="%TLK142" />
                  <TableColumn fx:id="brand" maxWidth="1.7976931348623157E308" minWidth="-1.0" prefWidth="58.0" text="%TLK143" />
                  <TableColumn fx:id="flightNumber" maxWidth="1.7976931348623157E308" minWidth="-1.0" prefWidth="75.0" text="%TLK144" />
                  <TableColumn fx:id="luggageTag" maxWidth="1.7976931348623157E308" minWidth="-1.0" prefWidth="75.0" text="%TLK145" />
                  <TableColumn fx:id="locationFound" maxWidth="1.7976931348623157E308" minWidth="-1.0" prefWidth="93.0" text="%TLK146" />
                  <TableColumn fx:id="mainColor" minWidth="0.0" prefWidth="113.0" text="%TLK147" />
                  <TableColumn fx:id="secondColor" maxWidth="1.7976931348623157E308" minWidth="-1.0" prefWidth="148.0" text="%TLK148" />
                  <TableColumn fx:id="size" prefWidth="75.0" text="%TLK149" />
                  <TableColumn fx:id="weight" prefWidth="75.0" text="%TLK150" />
                  <TableColumn fx:id="passNameAndCity" prefWidth="75.0" text="%TLK151" />
                  <TableColumn fx:id="charateristics" prefWidth="75.0" text="%TLK152" />
              </columns>
               <BorderPane.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </BorderPane.margin>
            </TableView>
         </center>
      </BorderPane>
   </children>
</AnchorPane>
